<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="@dimen/size20"
    tools:context=".presentation.BlankFragment">

    <com.example.tbcacademyhomework.presentation.core.views.input.InputFieldView
        android:id="@+id/et1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <com.example.tbcacademyhomework.presentation.core.views.input.InputFieldView
        android:id="@+id/et2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:startIcon="@drawable/ic_launcher_foreground"
        app:endIcon="@drawable/ic_visibility"
        />

    <com.example.tbcacademyhomework.presentation.core.views.input.InputFieldView
        android:id="@+id/et3"
        app:hint ="asdad"
        android:layout_width="match_parent"
        app:textColor="#ABAB21"
        android:layout_height="wrap_content"
        app:inputType="password"
        app:startIcon="@drawable/ic_launcher_foreground" />

    <Button
        android:id="@+id/btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

</androidx.appcompat.widget.LinearLayoutCompat>